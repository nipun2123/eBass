<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ebass">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.jobs.JobsFragment"
            android:label="@string/title_activity_jobs_fragment"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".ui.home.Problem_Actionbar" />
        <activity android:name=".ui.home.empty" />
        <activity android:name=".ui.home.Problem_RequestDetails" />
        <activity android:name=".ui.home.Problem_ConfirmRequest" />
        <activity android:name=".ui.home.Problem_EnterLocation" />
        <activity android:name=".ui.home.Problem_EnterProblem" />
        <activity android:name=".ui.home.Problem_SelectCompany" />
        <activity android:name=".ui.home.Problem_WhichWork" />
        <activity
            android:name=".MainBottomNavigation"
            android:label="@string/title_activity_main_bottom_navigation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>